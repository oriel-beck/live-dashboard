{
  "name": "api",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs -d ormconfig.ts",
    "migration:generate": "bun run typeorm migration:generate src/migrations/migration",
