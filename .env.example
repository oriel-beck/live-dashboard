# Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=discord_bot
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/discord_bot

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# API Configuration
NODE_ENV=development
CORS_ORIGIN=http://localhost:4200
API_PORT=3000

# Discord Bot Token (you'll need to set this)
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# Discord Client ID (you'll need to set this)
DISCORD_CLIENT_ID=your_discord_client_id_here

# Discord Client Secret (you'll need to set this)
DISCORD_CLIENT_SECRET=your_discord_client_secret_here

# JWT Secret (you'll need to set this)
JWT_SECRET=your_jwt_secret_here

# Session Secret (you'll need to set this)
SESSION_SECRET=your_session_secret_here
